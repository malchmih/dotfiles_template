source ~/.vimrc

nnoremap <leader>ef :action :cursive.repl.actions/run-top-sexp<cr>
nnoremap <leader>ee :action :cursive.repl.actions/run-last-sexp<cr>

nnoremap <space>ff :action GotoFile<cr>
nnoremap <space>fc :action GotoClass<cr>
nnoremap <space>fs :action GotoSymbol<cr>
nnoremap <space>fn :action GotoNamespace<cr>

set ideajoin
set clipboard+=unnamed